class maxArea{
    
    static int calculate_Area(Rectangle arr[]){
        
        // Your code here
        Rectangle obb= new Rectangle(0,0);
       int max = obb.area(); 
       for(int i=0;i<arr.length;i++)
       {
           obb=arr[i];
           max=Math.max(max,obb.area());
           
       }
       
       return max;

        
    }
    
}
