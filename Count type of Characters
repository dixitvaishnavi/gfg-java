class Sol
{
    int[] count (String s)
    {
        // your code here  
        int[] arr = {0,0,0,0};
       char ch = ' ';
       for(int i=0; i<s.length(); i++)
       {
           ch = s.charAt(i);
           if(ch>='A' && ch<='Z')
               arr[0]++;
           else if(ch>='a' && ch<='z')
               arr[1]++;
           else if(ch>='0' && ch<='9')
               arr[2]++;
           else
               arr[3]++;
       }
       return arr;


    }
}
