class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        //Write your code here
           long min=a[0],max=a[0];
       for(int i=0; i<a.length; i++){
           if(min>a[i]){
               min=a[i];
           }
           if(max<a[i]){
               max=a[i];
           }
       }
       pair ans=new pair(min,max);
       return ans;

    }
}
