class TestClass
{
    // Add your code here. Define private variables, Constructors to initialize 
    // variables with the given values and a method findMin() which prints the 
    // Minimum value of a$b.
    
    private int a,b;
   public TestClass(int a, int b)
   {
       this.a = a;
       this.b = b;
   }
   
   public void findMin() 
   {
       try{
           int []arr = new int[4];
           arr[0] = a + b;
           arr[1] = a - b;
           arr[2] = a * b;
           arr[3] = a / b;
           int min = Math.min( Math.min(arr[0], arr[1]), Math.min(arr[2], arr[3]));
           System.out.println(min);
       }
       catch (Exception e) 
       {
           System.out.println(0);
       }

}
}
